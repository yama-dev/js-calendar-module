    "webpack:develop": "webpack --progress --colors --watch",
    "webpack:build": "webpack --mode=production --progress --colors"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yama-dev/js-calendar-module"
  },
  "bugs": {
    "url": "https://github.com/yama-dev/js-calendar-module/issues",
    "email": "tatsuya.yamamoto69@gmail.com"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "cross-env": "^5.2.0",
    "npm-run-all": "^4.1.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "calendar": "^0.1.1",
    "js-parse-module": "0.0.3",
    "dayjs": "^1.8.35"
  },
  "babel": {
